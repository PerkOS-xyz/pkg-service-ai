{
  "name": "@perkos/service-ai",
  "version": "1.0.0",
  "description": "Multi-provider AI service abstraction with OpenRouter, Replicate, and OpenAI support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "openai",
    "openrouter",
    "replicate",
    "llm",
    "gpt",
    "image-generation",
    "transcription",
    "tts",
    "perkos"
  ],
  "author": "PerkOS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PerkOS-xyz/pkg-service-ai.git"
  },
  "dependencies": {
    "openai": "^4.0.0",
    "replicate": "^0.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
